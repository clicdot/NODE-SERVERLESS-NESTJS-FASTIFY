    "ts-loader": "^8.0.4",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint-config-semistandard": "^8.0.1",
    "typescript": "^4.0.3",
    "webpack-node-externals": "^2.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
