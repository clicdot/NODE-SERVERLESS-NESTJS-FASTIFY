    "aws-lambda-fastify": "^1.4.4",
    "aws-serverless-fastify": "^1.0.21",
    "cors": "^2.8.5",
    "fastify-helmet": "^5.0.3",
    "fastify-jwt": "^2.1.3",
    "fastify-swagger": "^3.4.0",
    "nest-router": "^1.0.9",
